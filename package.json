    "faker": "4.1.0",
    "fetch-jsonp": "^1.1.3",
    "md5": "2.2.1",
    "mousetrap": "^1.6.1",
    "normalize.css": "3.0.3",
    "polished": "^1.8.1",
    "react": "^16.0.0",
    "react-app-rewire-styled-components": "^3.0.0",
    "react-aux": "^1.1.0",
    "react-contenteditable": "^2.0.5",
    "react-dom": "^16.0.0",
    "react-live": "^1.7.1",
    "react-modal": "^3.0.0",
    "react-scripts": "1.0.14",
    "react-select": "^1.0.0-rc.10",
    "react-sound": "^0.8.0",
    "redux-devtools-extension": "^2.13.2",
