    "spectacle": "^2.0.0",
    "styled-components": "^2.2.3",
    "unique-string": "^1.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:ci": "cross-env CI=true react-app-rewired test --env=jsdom",
    "test:cover": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 3001 -s public",
    "storybook:build": "build-storybook -s public",
    "slides": "cross-env NODE_ENV=development node slides/server.js",
    "slides:clean": "rimraf slides/dist",
    "slides:build": "npm run slides:clean && cross-env NODE_ENV=production webpack --display-error-details --config slides/webpack.config.production.js",
    "gh-pages": "git subtree push --prefix slides origin gh-pages",
    "gh-pages:force": "git push origin `git subtree split --prefix slides master`:gh-pages --force",
    "gh-pages:refresh": "git push origin :gh-pages && git subtree push --prefix slides origin gh-pages"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-knobs": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/addon-notes": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "faker": "^4.1.0",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "jest-enzyme": "^4.0.1",
    "jest-html-reporter": "^0.5.3",
    "lodash": "^4.17.4",
    "markdown-loader": "^2.0.1",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-app-rewired": "^1.3.3",
    "react-test-renderer": "^16.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "redux-mock-store": "^1.3.0",
    "storybook-addon-jsx": "^4.2.0",
    "storybook-addon-specifications": "^2.1.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/stories/",
      "src/services/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary",
      "html"
    ]
  }
}
